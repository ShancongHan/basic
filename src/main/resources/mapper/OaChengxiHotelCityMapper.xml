<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.basic.dao.OaChengxiHotelCityDao">

    <select id="selectCityId" resultType="com.example.basic.entity.OaChengxiHotelCity">
        select city_id, sys_city_id
        from oa_chengxi_hotel_city
        where sys_country_id = 10000001
    </select>
</mapper>